{
  "name": "Anti Tests",
  "version": "1.6",
  "description": "Bypass for blur check on test sites!",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://*.testportal.pl/*", "https://*.testportal.net/*"],
      "all_frames": true,
      "js": ["js/testportal.js"]
    },
    {
      "matches": ["https://*.pazdro.net/*"],
      "all_frames": true,
      "js": ["js/pazdro.js"]
    }
  ]
}
